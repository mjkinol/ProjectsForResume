<AnchorPane prefHeight="700.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hockey.java.controller.SignupController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="400.0">
         <image>
            <Image url="@../../Images/background.jpeg" />
         </image>
         <effect>
            <GaussianBlur />
         </effect>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#fcfcfc" height="263.0" layoutX="-6.0" layoutY="-6.0" opacity="0.7" stroke="WHITE" strokeType="INSIDE" width="406.0">
         <effect>
            <GaussianBlur radius="63.0" />
         </effect>
      </Rectangle>
      <Label fx:id="appTitle" alignment="CENTER" layoutX="113.0" layoutY="72.0" prefHeight="28.0" prefWidth="175.0" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Hockey Master" textAlignment="CENTER">

         <font>
            <Font name="OLD SPORT 01 COLLEGE NCV" size="21.0" />
         </font>
         <effect>
            <DropShadow color="WHITE" height="65.36" radius="25.705" spread="0.23" width="39.46" />
         </effect></Label>
      <Label fx:id="pageTitle" alignment="CENTER" layoutX="147.0" layoutY="139.0" prefHeight="27.0" prefWidth="106.0" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Sign Up" textAlignment="CENTER">
         <font>
            <Font name="OLD SPORT 01 COLLEGE NCV" size="21.0" />
         </font>
         <effect>
            <DropShadow color="WHITE" height="20.18" radius="23.515" spread="0.72" width="75.88" />
         </effect></Label>
      <TextField fx:id="username" layoutX="117.0" layoutY="321.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="117.0" layoutY="361.0" promptText="Password" />
      <PasswordField fx:id="passwordc" layoutX="117.0" layoutY="400.0" promptText="Confirm Password" />
      <Button fx:id="signup" layoutX="161.0" layoutY="472.0" mnemonicParsing="false" onAction="#signup" prefHeight="27.0" prefWidth="84.0" scaleX="2.0" scaleY="2.0" text="Sign Up">
      	<font><Font name="HockeyFont" size="13.0" url="@../../Fonts/HockeyFont.ttf" />
		</font>
         <effect>
            <InnerShadow />
         </effect>
	  </Button>
      <Button fx:id="back" layoutX="161.0" layoutY="576.0" mnemonicParsing="false" onAction="#goBack" prefHeight="19.0" prefWidth="84.0" scaleX="2.0" scaleY="2.0" text="Back">
      	<font><Font name="HockeyFont" size="13.0" url="@../../Fonts/HockeyFont.ttf" />
		</font>
         <effect>
            <InnerShadow />
         </effect>
	  </Button>
      <Label fx:id="message" layoutX="14.0" layoutY="519.0" prefHeight="34.0" prefWidth="372.0" textAlignment="CENTER" textFill="#c30c0c">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
